#!/bin/bash
# ! KEEP THE DOUBLE-SLASHES !
# They're required for Windows compatibility.
devbox_docker exec				\
       --interactive				\
       --tty					\
       --user www-data				\
       --env SSH_AUTH_SOCK=//tmp/ssh-agent.sock	\
       --workdir //var/www/.ssh			\
       "$devbox_container"			\
       ssh-add "$@"
